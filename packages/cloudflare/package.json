{
  "name": "zastro-websockets-cloudflare",
  "version": "1.0.0",
  "type": "module",
  "description": "Use WebSockets in your Astro SSR Apps with Cloudflare Workers",
  "license": "MIT",
  "author": "Zach Handley",
  "keywords": [
    "astro",
    "astro-integration",
    "astro-adapter",
    "websocket",
    "ssr",
    "cloudflare",
    "workers"
  ],
  "homepage": "https://github.com/zachhandley/zastro-websockets",
  "repository": {
    "type": "git",
    "url": "https://github.com/zachhandley/zastro-websockets.git",
    "directory": "packages/cloudflare"
  },
  "bugs": {
    "url": "https://github.com/zachhandley/zastro-websockets/issues"
  },
  "main": "./dist/adapter/index.js",
  "types": "./dist/adapter/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/adapter/index.d.ts",
      "import": "./dist/adapter/index.js"
    },
    "./adapter": {
      "types": "./dist/adapter/index.d.ts",
      "import": "./dist/adapter/index.js"
    },
    "./server": {
      "types": "./dist/entrypoints/server.d.ts",
      "import": "./dist/entrypoints/server.js"
    },
    "./image-endpoint": {
      "types": "./dist/entrypoints/image-endpoint.d.ts",
      "import": "./dist/entrypoints/image-endpoint.js"
    },
    "./image-service": {
      "types": "./dist/entrypoints/image-service.d.ts",
      "import": "./dist/entrypoints/image-service.js"
    },
    "./middleware": {
      "types": "./dist/entrypoints/middleware.d.ts",
      "import": "./dist/entrypoints/middleware.js"
    },
    "./websocket": {
      "types": "./dist/adapter/websocket/index.d.ts",
      "import": "./dist/adapter/websocket/index.js"
    },
    "./websocket/stats": {
      "types": "./dist/adapter/websocket/stats.d.ts",
      "import": "./dist/adapter/websocket/stats.js"
    },
    "./websocket/connection-manager": {
      "types": "./dist/adapter/websocket/connection-manager.d.ts",
      "import": "./dist/adapter/websocket/connection-manager.js"
    },
    "./websocket/middleware": {
      "types": "./dist/adapter/websocket/middleware.d.ts",
      "import": "./dist/adapter/websocket/middleware.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@astrojs/internal-helpers": "^0.6.1",
    "@astrojs/underscore-redirects": "^1.0.0",
    "tinyglobby": "^0.2.13"
  },
  "devDependencies": {
    "astro": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^4.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "astro": "^4.0.0 || ^5.0.0",
    "vite": "^6.0.0",
    "wrangler": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "wrangler": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}